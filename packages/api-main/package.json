        "mailparser": "^2.7.1",
        "merge-graphql-schemas": "^1.5.8",
        "mongoose": "^5.4.6",
        "morgan": "^1.9.0",
        "mustache": "^2.3.0",
        "node-html-parser": "^1.1.15",
        "nodemon": "^1.18.3",
